		<preference>
			<name>anyo</name>
			<read-only>false</read-only>
		</preference></portlet-preferences></portlet>
	<user-attribute>
		<name>user.default.time_zone</name>
	</user-attribute>
	<user-attribute>
		<name>user.default.time.format</name>
	</user-attribute>
	<user-attribute>
		<name>user.default.date.format</name>
	</user-attribute>
	<user-attribute>
		<name>user.default.number.format</name>
	</user-attribute>
